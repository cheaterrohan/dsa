class Solution {
    public void moveZeroes(int[] nums) {
        int count =0;
        int k=0;
        for(int i =0;i<nums.length;i++){
            if(nums[i]==0){
                count++;
            }
            else{
                nums[k]=nums[i];
                k++;
            }
        }
        for(int i=0;i<count;i++){
            nums[k]=0;
            k++;
        }
    }
}
